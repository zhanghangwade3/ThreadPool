<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="org.lenve.threadpool.MainActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="btnClick"
        android:text="Hello World!"/>
    <Button
        android:layout_width="wrap_content"
        android:text="fixedThreadPool"
        android:onClick="fixedThreadPool"
        android:layout_height="wrap_content"/>
    <Button
        android:layout_width="wrap_content"
        android:text="SingleThreadPool"
        android:onClick="singleThreadPool"
        android:layout_height="wrap_content"/>
    <Button
        android:layout_width="wrap_content"
        android:text="cachedThreadPool"
        android:onClick="cachedThreadPool"
        android:layout_height="wrap_content"/>
    <Button
        android:layout_width="wrap_content"
        android:text="ScheduledThreadPool"
        android:onClick="scheduledThreadPool"
        android:layout_height="wrap_content"/>
    <Button
        android:layout_width="wrap_content"
        android:text="submit"
        android:onClick="submit"
        android:layout_height="wrap_content"/>
    <Button
        android:layout_width="wrap_content"
        android:text="自定义线程池"
        android:onClick="customThreadPool"
        android:layout_height="wrap_content"/>
</LinearLayout>
